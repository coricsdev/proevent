{
  "name": "proevent-theme",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/main.css --minify",
    "watch": "tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/main.css --watch",
    "lint": "eslint assets/js/**/*.js",
    "format": "prettier --write \"**/*.{js,css,scss,php,json,md}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@storybook/addon-webpack5-compiler-swc": "^4.0.2",
    "@storybook/react-webpack5": "^10.0.8",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.5.0",
    "eslint-plugin-storybook": "^10.0.8",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "storybook": "^10.0.8",
    "tailwindcss": "^3.4.18"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
