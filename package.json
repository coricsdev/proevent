{
    "name": "proevent-theme",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/main.css --minify",
        "watch": "tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/main.css --watch",
        "lint": "eslint assets/js/**/*.js",
        "format": "prettier --write \"**/*.{js,css,scss,php,json,md}\""
    },
    "devDependencies": {
        "autoprefixer": "^10.4.22",
        "eslint": "^9.5.0",
        "postcss": "^8.5.6",
        "prettier": "^3.3.3",
        "tailwindcss": "^3.4.18"
    }
}
